{
  "version": 2,
  "framework": "angular",
  "rewrites": [
    { "source": "/:path*", "destination": "/index.html" },
    { "source": "/", "destination": "/index.html" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "outputDirectory": "dist/smart/browser",
  "cleanUrls": true,
  "trailingSlash": false
}
